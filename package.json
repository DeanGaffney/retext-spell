{
  "name": "retext-spell",
  "version": "0.0.0",
  "description": "Spelling checker for retext",
  "license": "MIT",
  "keywords": [
    "spelling",
    "correct",
    "checker",
    "spell",
    "fix",
    "autocorrect",
    "retext"
  ],
  "repository": "https://github.com/wooorm/retext-spell",
  "bugs": "https://github.com/wooorm/retext-spell/issues",
  "author": "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)",
    "Josiah Sprague <josiah.sprague@gmail.com>",
    "Jan Wirth <jottenwe@gmail.com>"
  ],
  "engines": {
    "node": ">=0.12.0"
  },
  "files": [
    "index.js"
  ],
  "dependencies": {
    "hunspell-spellchecker": "^1.0.2",
    "lodash.includes": "^4.2.0",
    "nlcst-is-literal": "^1.0.0",
    "nlcst-to-string": "^2.0.0",
    "unist-util-visit": "^1.0.0"
  },
  "devDependencies": {
    "dictionary-en-gb": "^1.0.1",
    "dictionary-en-us": "^1.0.1",
    "nyc": "^10.0.0",
    "remark-cli": "^2.0.0",
    "remark-preset-wooorm": "^1.0.0",
    "retext": "^4.0.0",
    "tape": "^4.2.0",
    "xo": "^0.17.1"
  },
  "scripts": {
    "build-md": "remark . -qfo",
    "build": "npm run build-md",
    "lint": "xo",
    "test-api": "node test",
    "test-coverage": "nyc --reporter lcov tape test.js",
    "test": "npm run build && npm run lint && npm run test-coverage"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "xo": {
    "space": true,
    "ignores": [
      "retext-spell.js"
    ]
  },
  "remarkConfig": {
    "presets": "wooorm"
  }
}
